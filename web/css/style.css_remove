.topheader a.tabs_button {background:transparent url(../images/top_btn_a.png) no-repeat scroll top right;color: #ffffff;display: block;float: left;font:normal 13px arial, sans-serif;
    height:26px;margin-right: 6px;padding-right: 12px; /* sliding doors padding */text-decoration: none;}
.topheader a.tabs_button span {background: transparent url(../images/top_btn_span.png) no-repeat;display: block;line-height:26px;padding: 0 0 0px 12px;} 
.topheader a.tabs_button:hover {background-position: bottom right;color: #000000;outline: none;padding-right: 12px; /* hide dotted outline in Firefox */}
.topheader a.tabs_button:hover span {background-position: bottom left;padding: 0 0 0 12px; /* push text down 1px */}

