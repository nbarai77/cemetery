#main_menu { margin:10px 0; padding:0;}
#main_menu .menu {padding:0; margin:0; list-style:none; background:url(../images/main_menu_middle.png) repeat-x; height:50px; width:973px; position:relative; z-index:500; font-family:arial, verdana, sans-serif; float:left;}
#main_menu .menu li {float:left; margin-right:1px;}
#main_menu .menu li a {display:block; float:left; height:50px; line-height:50px; background:none; color:#fff; text-decoration:none; font-size:14px; font-weight:normal; padding:0 18px;}
#main_menu .menu table {border-collapse:collapse; width:0; height:0; position:absolute; top:0; left:0;}
#main_menu .menu li a:hover {z-index:200; position:relative;color:#fff; background-color:#c60;}
#main_menu .menu li:hover {position:relative; z-index:200;}
#main_menu .menu li:hover > a {color:#fff; background:#c60;}
#main_menu .menu li:hover > a.sub {color:#fff; background-color:#c60;}
#main_menu .menu li.current a {color:#fff; background:#c60;}
#main_menu .menu li a.sub {background: # url(../images/down-arrow.gif) no-repeat right center;}
#main_menu .menuli.current a.sub {color:#fff; background:#840 url(../images/down-arrow.gif) no-repeat right center;}
#main_menu .menu :hover ul {left:0; top:50px; width:150px; background:#444;}
#main_menu .menu ul, 
#main_menu .menu :hover ul ul {position:absolute; left:-9999px; top:-9999px; width:0; height:0; margin:0; padding:0; list-style:none;}
#main_menu .menu :hover ul :hover ul{left:120px; top:-1px; background:#222; white-space:nowrap; width:150px; z-index:200; height:auto;}
#main_menu .menu :hover ul li {margin:0; border-top:1px solid #666; width:150px;}
#main_menu .menu :hover ul li a {width:150px; padding:0; text-indent:10px; background:#006b8f; color:#fff; height:30px; line-height:30px;}
#main_menu .menu :hover ul li a.fly {background:#333 url(../images/right-arrow.gif) no-repeat right center;}
#main_menu .menu :hover ul :hover {background-color:#c60; color:#fff;}
#main_menu .menu :hover ul :hover a.fly {background-color:#c60; color:#fff;}
#main_menu .menu :hover ul li.currentsub a {background:#840; color:#fff;}
#main_menu .menu :hover ul li.currentsub a.fly {background:#840 url(../images/right-arrow.gif) no-repeat right center; color:#fff;}
#main_menu .menu :hover ul :hover ul li a {width:100px; padding:0; text-indent:10px; background:#3e3e3e; color:#ccc;}
#main_menu .menu :hover ul :hover ul :hover {background-color:#d70; color:#fff;}
#main_menu .menu :hover ul :hover ul li.currentfly a,
#main_menu .menu :hover ul :hover ul li.currentfly a:hover {background:#840; color:#fff;}


